         return "";// 15
      }

      @JvmSynthetic
      open fun A(`$constructor_marker`: DefaultConstructorMarker) {
         this();
      }

      class B : TestSmartCasts.A {
         fun B() {
            super(null);// 8
         }

         fun testB(): String {
            return "B";// 9
         }
      }

      class C : TestSmartCasts.A {
         fun C() {
            super(null);// 11
         }

         fun testC(): String {
            return "C";// 12
         }
      }
   }

   interface X {
