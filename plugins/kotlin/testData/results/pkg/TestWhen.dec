public class TestWhen {
   public fun testStatement(obj: Any) {
      if (obj == 1) {// 5 6
         System.out.println("1");
      } else if (obj == "2") {// 7
         System.out.println("2");
      } else if (obj is java.lang.Double) {// 8
         System.out.println("Double");
      } else if (obj !is java.lang.Long) {// 9
         System.out.println("Not Long");
      } else {
         System.out.println("else");// 10
      }
   }// 12

   public fun testStatement2(a: Any, b: Any) {
      if (a == 15) {// 16
         System.out.println("a == 15");
      } else if (a == "!!") {// 17
         System.out.println("a == !!");
      } else if (a is Int) {// 18
         System.out.println("a is Int");
      } else if (a is java.lang.String) {// 19
         System.out.println("a is String");
      } else if (b is java.lang.Double) {// 20
         System.out.println("b is Double");
      } else if (a is Unit) {// 21
         System.out.println("a is Unit");
      } else {
         System.out.println("else");// 22
      }
   }// 24

   public fun testExpression(obj: Any): Int {
      return if (obj == 1) 1 else (if (obj is java.lang.Double) 2 else (if (obj == "4") 4 else (if (obj !is java.lang.Long) 3 else 5)));// 27 28 29 30 31 32
   }

   public fun testExpression2(obj: Any): Int {
      val text: java.lang.String = if (obj == 1)// 37 38
         "one"
         else
         (if (obj is java.lang.Double) "double" else (if (obj == "4") "four" else (if (obj !is java.lang.Long) "not long" else "something")));// 39 40 41 42
      System.out.println(text);// 44
      return text.length();// 45
   }

   public fun testSwitchExpression(a: Int) {
      var var10000: java.lang.String;
      when (a) {// 49
         1 -> var10000 = "one";// 50
         2, 3, 5 -> var10000 = "prime";// 51
         4 -> var10000 = "something";// 52
         else -> something";
      }

      System.out.println(var10000);// 54
   }// 55

   public fun testCompilesToTableSwitch(a: Int) {
      when (a) {// 58
         1 -> System.out.println("The loneliest number");// 59
         2, 3 -> System.out.println("Small prime");// 60
         else -> System.out.println("Number");// 61
      }
   }// 63

   public fun testCompilesToLookupSwitch(a: Int) {
      when (a) {// 66
         1 -> System.out.println("Go for gold");// 67
         9 -> System.out.println("The cat's meow");// 68
         42 -> System.out.println("The answer");// 69
         else -> System.out.println("Number");// 70
      }
   }// 72

   public fun booleanNightmares(a: Boolean, b: Boolean, c: Boolean, d: Boolean, e: Boolean, f: Boolean, g: Boolean) {
      if (a == (b != c)) {// 75 76
         System.out.println("-_-");
      } else if (a == (b && !e)) {// 77
         System.out.println("xxx");
      } else if (a == (!a || d)) {// 78
         System.out.println("ohno");
      } else if (!a) {// 79
         System.out.println("NIGHTMARE");
      } else if (a == (g || e && f != c)) {// 80
         System.out.println("hello");
      } else {
         System.out.println("else");// 81
      }
   }// 83
}

