   public void test1(int i, int j) {
      int b = ++i * j;// 11 12
      System.out.println(b);// 13
   }// 14

   public void test2(int i, int j) {
      i++;// 17
      int b = i * j / i;// 18
      System.out.println(b);// 19
   }// 20
}

